#!/usr/bin/env node
'use strict';

process.title = 'ALKS';

var program = require('commander'),
    utils   = require('../lib/utils'),
    config  = require('../package.json');

program
    .version(config.version)
    .command('create', 'creates a new session')
    .command('list',   'lists previous keys')
    .parse(process.argv);